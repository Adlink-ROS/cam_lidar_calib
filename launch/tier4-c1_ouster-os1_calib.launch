<?xml version="1.0" encoding="utf-8"?>
<launch>

  <arg name="use_sim_time" default="false" />

  <node pkg="cam_lidar_calib" name="cam_lidar_calib" type="cam_lidar_calib_node" output="screen">
    <param name="use_sim_time" value="$(arg use_sim_time)"/>
  	<param name="dx" type="double" value="0.065"/>
  	<param name="dy" type="double" value="0.065"/>
  	<param name="checkerboard_rows" type="int" value="11" />
  	<param name="checkerboard_cols" type="int" value= "8" />
  	<param name="result_file" type="string" value="$(find cam_lidar_calib)/result/EW2023/calibated_result.txt"/>
  	<param name="result_rpy_file" type="string" value="$(find cam_lidar_calib)/result/EW2023/rpy_txyz_calibated_result.txt"/>
  	<param name="min_points_on_plane" type="int" value="700"/>
  	<param name="num_views" type="int" value="20"/>
  	<param name="camera_in_topic" type="string" value="/port_1/camera/image_raw"/>
  	<param name="lidar_in_topic" type="string" value="/ouster/points"/>
	<param name="no_of_initializations" type="int" value="1"/>
    <param name="initializations_file" value="$(find cam_lidar_calib)/result/EW2023/init_file.txt"/>
	<param name="x_min" value="0.75"/>
    <param name="x_max" value="6"/>
    <param name="y_min" value="-2"/>
    <param name="y_max" value="2"/>
    <param name="z_min" value="-1"/>
    <param name="z_max" value="3"/>
    <param name="ransac_threshold" value="0.01"/>
  	<param name="cam_config_file_path" type="string" value="$(find cam_lidar_calib)/config/tier4_c1.yaml"/>
  </node>

</launch>
